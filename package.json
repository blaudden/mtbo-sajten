{
  "name": "mtbo-sajten",
  "description": "Package for MTBO sajten mountainbikeorientering.se.",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >= 21.0.0"
  },
  "scripts": {
    "dev": "astro dev --host 127.0.0.1",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "npm run check:astro && npm run check:eslint && npm run check:prettier",
    "check:astro": "astro check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier -w .",
    "test": "npm run test:og && npm run test:meta && npm run test:hreflang",
    "test:og": "node tests/verify-og-images.mjs",
    "test:meta": "node tests/verify-meta-descriptions.mjs",
    "test:hreflang": "node tests/verify-hreflang.mjs",
    "validate:links": "node tests/check-links.mjs",
    "optimize-images": "node scripts/optimize-images.mjs",
    "validate:images": "node scripts/validate-images.mjs",
    "submit:indexnow": "node scripts/submit-indexnow.js",
    "fetch-youtube": "tsx scripts/fetch-youtube.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/markdoc": "^0.15.10",
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.1",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@fontsource-variable/inter": "^5.1.1",
    "@keystatic/astro": "^5.0.6",
    "@keystatic/core": "^0.5.48",
    "@types/leaflet": "^1.9.21",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.10",
    "astro-embed": "^0.12.0",
    "astro-icon": "^1.1.5",
    "leaflet": "^1.9.4",
    "limax": "4.2.1",
    "lodash.merge": "^4.6.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/tailwind": "^5.1.5",
    "@distube/ytdl-core": "^4.15.9",
    "@eslint/js": "^9.39.2",
    "@iconify-json/flat-color-icons": "^1.2.1",
    "@iconify-json/tabler": "^1.2.26",
    "@tailwindcss/typography": "^0.5.16",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/mdx": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "astro-compress": "2.3.8",
    "astro-eslint-parser": "^1.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.3.1",
    "fluent-ffmpeg": "^2.1.3",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.4.1",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
